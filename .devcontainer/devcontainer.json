{
    "name": "DesterLib",
    "onCreateCommand": "bash scripts/setup.sh",
    "forwardPorts": [
        35500,
        35510,
        35530
    ],
    "customizations": {
        "codespaces": {
            "repositories": {
                "DesterLib/Server": {
                    "permissions": {
                        "contents": "write",
                        "pull_requests": "write"
                    }
                },
                "DesterLib/Frontend": {
                    "permissions": {
                        "contents": "write",
                        "pull_requests": "write"
                    }
                }
            }
        }
    },
    "extensions": [
        "mikestead.dotenv",
        "dsznajder.es7-react-js-snippets",
        "dbaeumer.vscode-eslint",
        "VisualStudioExptTeam.vscodeintellicode",
        "esbenp.prettier-vscode",
        "ms-python.vscode-pylance",
        "ms-python.python",
        "eamodio.gitlens"
    ]
}
