{
    "name": "Dester",
    "onCreateCommand": "bash scripts/setup.sh",
    "forwardPorts": [
        35500,
        35510,
        35530
    ],
    "portsAttributes": {
        "35500": {
            "label": "Backend"
        },
        "35510": {
            "label": "Frontend"
        },
        "35530": {
            "label": "Rclone"
        }
    },
    "customizations": {
        "codespaces": {
            "repositories": {
                "DesterLib/Backend": {
                    "permissions": {
                        "contents": "write",
                        "pull_requests": "write"
                    }
                },
                "DesterLib/Frontend": {
                    "permissions": {
                        "contents": "write",
                        "pull_requests": "write"
                    }
                },
                "DesterLib/Docs": {
                    "permissions": {
                        "contents": "write",
                        "pull_requests": "write"
                    }
                }
            }
        }
    },
    "extensions": [
        "mikestead.dotenv",
        "dsznajder.es7-react-js-snippets",
        "dbaeumer.vscode-eslint",
        "VisualStudioExptTeam.vscodeintellicode",
        "esbenp.prettier-vscode",
        "ms-python.vscode-pylance",
        "ms-python.python",
        "eamodio.gitlens"
    ]
}